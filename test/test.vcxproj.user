<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <RemoteDebuggerCommand>$(RemoteRootDir)/bin/$(Platform)/$(Configuration)/$(TargetName)$(TargetExt)</RemoteDebuggerCommand>
    <RemoteDebuggerWorkingDirectory>$(RemoteRootDir)/bin/$(Platform)/$(Configuration)</RemoteDebuggerWorkingDirectory>
    <AdditionalDebuggerCommands>set solib-search-path $(SolutionDir)bin/$(Platform)/$(Configuration)</AdditionalDebuggerCommands>
    <DebuggerFlavor>LinuxDebugger</DebuggerFlavor>
    <RemoteDebuggingMode>gdbserver</RemoteDebuggingMode>
  </PropertyGroup>
</Project>